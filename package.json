{
  "name": "@getcommunity/client-styleguides",
  "description": "A collection of reusable styles for GetCommunity client projects.",
  "version": "0.0.15",
  "author": "@GetCommunity <ryan@getcommunity.com> (https://github.com/GetCommunity)",
  "contributors": [
    "Joey Grable <joey@getcommunity.com> (https://joeygrable.com)"
  ],
  "dependencies": {
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "@getcommunity/config-eslint": "*",
    "@getcommunity/config-prettier": "*",
    "@getcommunity/config-typescript": "*",
    "@types/node": "^24.0.15",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "solid-js": "^1.9.7",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^5.4.11"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "license": "ISC",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "getcommunity/client-styleguides.git",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev:preview": "vite serve dev",
    "dev:watch": "tsup --watch",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "lint": "eslint ./src",
    "typecheck": "tsc --noEmit --jsx preserve ./src/**/*"
  },
  "sideEffects": false,
  "source": "./src/index.ts",
  "type": "module",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "./dist/index.d.ts"
      ]
    }
  }
}
